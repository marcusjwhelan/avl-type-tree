{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "declaration": true,
        "importHelpers": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "emitDecoratorMetadata": true,
        "noEmitOnError": true,
        "experimentalDecorators": true
    }
}
